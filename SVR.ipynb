   ],
   "source": [
    "x = [[1],[2],[3],[4],[5]]\n",
    "y = [2.1, 4.3, 3.2, 5.2, 6.3]\n",
    "\n",
    "model = SVR(kernel = 'rbf')\n",
    "model.fit(x,y)\n",
    "\n",
    "y_pred = model.predict(x)\n",
    "\n",
    "mse = mean_squared_error(y, y_pred)\n",
    "mse"
   ]
